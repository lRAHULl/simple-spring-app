{
    "family": "simple-java-app",
    "placementConstraints": [],
    "volumes": [],
    "requiresCompatibilities": [
        "EC2"
    ],
    "containerDefinitions": [
        {
            "memoryReservation": 128,
            "name": "simple-java-app",
            "mountPoints": [],
            "image": "DOCKER_IMAGE_NAME",
            "essential": true,
            "portMappings": [
                {
                    "protocol": "tcp",
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "test",
                    "awslogs-group": "/ecs/simple-java-app"
                }
            },
            "memory": 1024,
            "cpu": 1,
            "volumesFrom": []
        }
    ]
}
